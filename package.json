{
  "name": "wade-pate-dev-portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "yarn db:generate && next build",
    "vercel-build": "yarn db:generate && next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "yarn pnpify prisma generate",
    "db:push": "yarn pnpify prisma db push",
    "db:migrate": "yarn pnpify prisma migrate dev",
    "db:init": "yarn pnpify prisma db push --skip-generate",
    "db:seed": "NODE_ENV=development tsx prisma/seed.ts",
    "db:seed-blog": "tsx scripts/seed-blog.ts",
    "db:seed-production": "NODE_ENV=production tsx prisma/seed.ts",
    "db:cleanup": "tsx scripts/cleanup-sample-data.ts",
    "db:studio": "yarn pnpify prisma studio",
    "docker:build": "docker build -t portfolio .",
    "docker:run": "docker run -p 3000:3000 portfolio",
    "docker:dev": "docker-compose -p portfolio --profile dev up --build",
    "docker:prod": "docker-compose -p portfolio up --build",
    "docker:prod-full": "docker-compose -f docker-compose.production.yml up --build -d",
    "docker:logs": "docker-compose -f docker-compose.production.yml logs -f",
    "docker:down": "docker-compose -f docker-compose.production.yml down",
    "deploy": "./scripts/deployment/deploy-production.sh",
    "deploy:vercel": "DEPLOY_TARGET=vercel ./scripts/deployment/deploy-production.sh",
    "type-check": "tsc --noEmit",
    "analyze": "ANALYZE=true yarn build"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@next/env": "^15.4.5",
    "@prisma/client": "6.13.0",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@vercel/analytics": "^1.5.0",
    "@yarnpkg/pnpify": "^4.1.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.6",
    "jose": "^6.0.12",
    "lucide-react": "^0.536.0",
    "next": "15.4.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "resolve": "^1.22.10",
    "tailwind-merge": "^3.3.1",
    "tsx": "^4.20.3",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/eslint-plugin-next": "^15.4.5",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20.19.8",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/resolve": "^1",
    "eslint": "^9",
    "eslint-config-next": "15.4.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "prisma": "6.13.0",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.6",
    "typanion": "^3.14.0",
    "typescript": "^5"
  },
  "packageManager": "yarn@4.9.2+sha512.1fc009bc09d13cfd0e19efa44cbfc2b9cf6ca61482725eb35bbc5e257e093ebf4130db6dfe15d604ff4b79efd8e1e8e99b25fa7d0a6197c9f9826358d4d65c3c"
}
